#include <stdio.h>
int main() {
    int a,b;
    printf("Enter the a value :\n");
    scanf("%d%d",&a,&b);
    a+=b;
    printf("Add%d\n",a);
    a-=b;
    printf("Sub%d\n",a);
    a*=b;
    printf("Mul%d\n",a);
    a/=b;
    printf("Div%d\n",a);
    a%=b;
    printf("mod%d\n",a);
    
    return 0;
}
