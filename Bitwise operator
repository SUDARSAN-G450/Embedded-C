#include <stdio.h>

int main() {
    int a,b;
    printf("Enter a value");
    scanf("%d",&a);
    printf("Enter b value");
    scanf("%d",&b);
    printf("and%d\n",a&b);
    printf("or%d\n",a|b);
    printf("not%d\n",~a);
    printf("ex-or%d\n",a^b);
    printf("left shift%d\n",a<<b);
    printf("right shift%d\n",a>>b);
    return 0;
}
